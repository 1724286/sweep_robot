<launch>
    <!--- Sim Time -->
    <param name="/use_sim_time" value="false" />

    <!--- Run Rviz  args="-d $(find test_ros)/launch/test.rviz" -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find sweepRobot)/launch/my.rviz" />

    <node pkg="sweepRobot" type="sweepRobotnode"    name="ddsdfs"    output="screen"/>
    <node pkg="sweepRobot" type="keyboard_node"    name="ssdfds"    output="screen"/>
    <node pkg="sweepRobot" type="stereo_vo_node"    name="dwada"    output="screen"/>

    <node pkg="rosbag" type="record" name="bag_record" args="/robot_imu /robot_stereo"/>

</launch>
