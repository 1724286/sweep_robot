    <node pkg="sweepRobot" type="stereo_vo_node"    name="dwada"    output="screen"/>

    <node pkg="rosbag" type="record" name="bag_record" args="/imu0 /cam0/image_raw /cam1/image_raw"/>
<!--     <node pkg="rosbag" type="record" name="bag_record" args=" -duration=121m /imu0 "/> -->

</launch>

